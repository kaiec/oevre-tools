class Log
  constructor: (@write) ->
    if @write==undefined then @write = (out)->


  log: (level, message) ->
    @write(level + ": " + message)

  info: (message) ->
    @log("INFO", message)

  error: (message) ->
    @log("ERROR", message)

  fine: (message) ->
    @log("FINE", message)

  setWriter: (writer) ->
    @write = writer

module.exports = new Log()